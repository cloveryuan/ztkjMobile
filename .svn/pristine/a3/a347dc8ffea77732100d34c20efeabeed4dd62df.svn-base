:global{
    .am-navbar{
        background-color: #248bd1;
    }
    .am-grid .am-flexbox{
        background: none;
    }
    .am-list-item {
        background-color: #606386;
        float: left;
    }
    @media (-webkit-min-device-pixel-ratio: 2), not all, (min-resolution: 2dppx){
        html:not([data-scale]) .am-list-body div:not(:last-child) .am-list-line::after {
            height: 0 !important;
        }
        html:not([data-scale]) .am-list-body::before {
          height: 0 !important;
      }

    }
    p{
        margin-bottom: 0 !important;
    }
    .am-list-item .am-list-line {
        padding-right: 0 !important;
        border: none !important;
    }
    .am-list-body {
        border: none !important;  
  }

}
.main{
    background: url(./../../image/bg_01.png) no-repeat;
}
.main_title {
    background: url(./../../image/hd_01.png) no-repeat;
    border-bottom: 1px solid rgba(255,255,255,0.3);
    width: 100%;
    color: rgb(255, 255, 255);
    font-size: small;
    font-weight: 500;
    line-height: 48px;
    padding-left: 16px;
}
  
  .div_top {
    padding: 0 8px 0 0;
    display: inline-block;
  }
  
  .div_lr {
    margin: 6px 0;
    position: relative;
  }
  
  .div_mt {
    margin: 14px 0 0 0
  }
  
  .div_pt {
    padding: 14px 0 0 0
  }
  
  .div_radius {
    position: absolute;
    top: -5px;
    right: 5px;
  }
  
  .border_l {
    border-left: 1px solid rgba(255, 255, 255, 1);
  }
  
  .border_t {
    border-top: 1px solid rgba(255, 255, 255, 1);
  }
  
  .border_r {
    border-right: 1px solid rgba(255, 255, 255, 1);
    position: relative;
    margin: 6px 0;
  }
  
  .border_b {
    border-bottom: 1px solid rgba(255, 255, 255, 1);
  }
  
  .border_b2 {
    border-top: 1px solid rgba(143, 158, 187, 1);
    border-bottom: 1px solid rgba(143, 158, 187, 1);
  }
  
  .h_font {
    font-size: 16px;
    color: white;
  }
  
  .h1_font {
    font-size: 22px;
    font-weight: bold;
    color: #00D795;
    display: inline-block;
  }
  
  .h2_font {
    font-size: 16px;
    color: #FFFFFF;
    display: inline-block;
  }
  
  .back_ground {
    background: rgba(255, 255, 255, 0.2);
  }
  
  .img_size {
    width: 40px;
    height: 40px;
  }
  .icon_font{
    font-size: 10px;
    color: white;
  }
  .echarts_font{
    padding-left: 10px;
    color: #FFFFFF
  }
  
  .echarts_pie {
    width: 100%;
    height: 200px;
  }
  